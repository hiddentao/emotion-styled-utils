{
  "name": "emotion-styled-utils",
  "version": "2.2.0",
  "description": "Utilities for working with emotion and styled components.",
  "scripts": {
    "clean": "rm -rf ./dist && mkdir ./dist",
    "compile": "babel ./src --out-dir ./dist",
    "build": "npm run clean && npm run compile",
    "watch": "npm run clean && npm run compile -- --watch"
  },
  "keywords": [
    "styling",
    "emotion"
  ],
  "files": [
    "dist"
  ],
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiddentao/emotion-styled-utils.git"
  },
  "bugs": {
    "url": "https://github.com/hiddentao/emotion-styled-utils/issues"
  },
  "homepage": "https://solui.dev",
  "dependencies": {
    "color": "^3.1.2",
    "fontfaceobserver": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-proposal-export-default-from": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.6.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1"
  }
}
